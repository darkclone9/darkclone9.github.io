{
  "codeTemplates": {
    "version": "1.0.0",
    "categories": {
      "comments": {
        "highDensityFunction": {
          "pattern": "/**\n * @purpose: {purpose}\n * @dependencies: {dependencies}\n * @complexity: {timeComplexity}, {spaceComplexity}\n * @side-effects: {sideEffects}\n * @ai-context: {{purpose: \"{contextPurpose}\", pattern: \"{pattern}\", complexity: \"{complexity}\"}}\n */",
          "variables": {
            "purpose": "string",
            "dependencies": "array",
            "timeComplexity": "string",
            "spaceComplexity": "string",
            "sideEffects": "string",
            "contextPurpose": "string",
            "pattern": "string",
            "complexity": "enum:low,medium,high"
          },
          "autoGenerate": true
        },
        "aiGenerationMarker": {
          "pattern": "// @ai-generated: true\n// @review-required: {reviewTypes}\n// @generated-at: {timestamp}\n// @confidence: {confidence}",
          "variables": {
            "reviewTypes": "array",
            "timestamp": "datetime",
            "confidence": "enum:low,medium,high"
          },
          "autoGenerate": true
        },
        "reviewCheckpoint": {
          "pattern": "// @review-checkpoint: {{type: \"{type}\", priority: \"{priority}\", reviewer: \"{reviewer}\"}}\n// TODO: {reviewDescription}",
          "variables": {
            "type": "enum:security,performance,business-logic,integration",
            "priority": "enum:critical,high,medium,low",
            "reviewer": "string",
            "reviewDescription": "string"
          },
          "autoGenerate": false
        }
      },
      "functions": {
        "performanceOptimized": {
          "pattern": "/**\n * @complexity: Time {timeComplexity}, Space {spaceComplexity}\n * @performance: {performanceNote}\n * @benchmark: {benchmark}\n */\n{asyncKeyword}function {functionName}({parameters}): {returnType} {\n  {implementation}\n}",
          "variables": {
            "timeComplexity": "string",
            "spaceComplexity": "string",
            "performanceNote": "string",
            "benchmark": "string",
            "asyncKeyword": "optional:async ",
            "functionName": "string",
            "parameters": "string",
            "returnType": "string",
            "implementation": "string"
          },
          "validations": {
            "timeComplexity": "^O\\([^)]+\\)$",
            "spaceComplexity": "^O\\([^)]+\\)$"
          }
        },
        "crudOperation": {
          "pattern": "/**\n * @ai-context: {{purpose: \"data-{operation}\", pattern: \"crud\", complexity: \"low\"}}\n * @input: {inputDescription}\n * @output: {outputDescription}\n * @throws: {throwsDescription}\n */\nasync {operation}{entity}({parameters}): Promise<{returnType}> {\n  // @ai-generated: true\n  // @review-required: [\"data-validation\", \"error-handling\"]\n  {implementation}\n}",
          "variables": {
            "operation": "enum:create,read,update,delete",
            "entity": "string",
            "parameters": "string",
            "returnType": "string",
            "inputDescription": "string",
            "outputDescription": "string",
            "throwsDescription": "string",
            "implementation": "string"
          }
        }
      },
      "classes": {
        "serviceClass": {
          "pattern": "/**\n * @ai-context: {{purpose: \"{purpose}\", pattern: \"service\", complexity: \"medium\"}}\n * @dependencies: {dependencies}\n */\nclass {className} {\n  constructor(\n    {constructorParams}\n  ) {}\n\n  {methods}\n}",
          "variables": {
            "purpose": "string",
            "dependencies": "array",
            "className": "string",
            "constructorParams": "string",
            "methods": "string"
          },
          "namingConvention": {
            "className": "{Domain}{Purpose}Service"
          }
        },
        "repositoryClass": {
          "pattern": "/**\n * @ai-context: {{purpose: \"data-access\", pattern: \"repository\", complexity: \"medium\"}}\n * @extends: BaseRepository<{entity}>\n */\nclass {className} extends BaseRepository<{entity}> {\n  // @inherits: [validation, errorHandling, logging]\n  \n  {customMethods}\n}",
          "variables": {
            "entity": "string",
            "className": "string",
            "customMethods": "string"
          },
          "namingConvention": {
            "className": "{Entity}Repository"
          }
        }
      },
      "interfaces": {
        "dependencyInterface": {
          "pattern": "/**\n * @ai-context: {{purpose: \"dependency-definition\", pattern: \"interface\", complexity: \"low\"}}\n */\ninterface {interfaceName}Dependencies {\n  readonly {dependencies}\n}",
          "variables": {
            "interfaceName": "string",
            "dependencies": "string"
          },
          "namingConvention": {
            "interfaceName": "{ServiceName}Dependencies"
          }
        },
        "crudInterface": {
          "pattern": "/**\n * @ai-context: {{purpose: \"crud-operations\", pattern: \"interface\", complexity: \"low\"}}\n * @template: T - Entity type, K - Key type\n */\ninterface {interfaceName}<T, K> {\n  create(entity: Omit<T, 'id'>): Promise<T>;\n  read(id: K): Promise<T | null>;\n  update(id: K, updates: Partial<T>): Promise<T>;\n  delete(id: K): Promise<boolean>;\n  list(filters?: Partial<T>): Promise<T[]>;\n}",
          "variables": {
            "interfaceName": "string"
          },
          "namingConvention": {
            "interfaceName": "I{Entity}Repository"
          }
        }
      },
      "types": {
        "dataFlowType": {
          "pattern": "/**\n * @ai-context: {{purpose: \"data-flow\", pattern: \"type\", complexity: \"low\"}}\n */\ntype {typeName}<Input, Output> = {\n  readonly input: Input;\n  readonly transform: (input: Input) => Output;\n  readonly output: Output;\n};",
          "variables": {
            "typeName": "string"
          },
          "namingConvention": {
            "typeName": "T{Purpose}Flow"
          }
        },
        "validationResult": {
          "pattern": "/**\n * @ai-context: {{purpose: \"validation-result\", pattern: \"type\", complexity: \"low\"}}\n */\ntype ValidationResult<T> = {\n  readonly valid: boolean;\n  readonly data?: T;\n  readonly errors: string[];\n  readonly warnings?: string[];\n};",
          "autoGenerate": true
        }
      },
      "decorators": {
        "performanceDecorator": {
          "pattern": "@{decoratorName}({{ {options} }})",
          "variables": {
            "decoratorName": "enum:Memoize,RateLimit,LazyLoad,Cache",
            "options": "string"
          },
          "examples": {
            "Memoize": "ttl: 300000",
            "RateLimit": "maxCalls: 100, window: 60000",
            "LazyLoad": "",
            "Cache": "key: 'user-{id}', ttl: 600"
          }
        },
        "reviewDecorator": {
          "pattern": "@ReviewRequired('{reviewType}')",
          "variables": {
            "reviewType": "enum:security,performance,business-logic,integration"
          }
        }
      },
      "patterns": {
        "errorHandling": {
          "pattern": "try {\n  {mainLogic}\n} catch (error) {\n  // @ai-generated: error handling\n  this.logger.error('{operation} failed', {{ error, context: {context} }});\n  \n  if (error instanceof {specificError}) {\n    {specificHandling}\n  }\n  \n  throw new {customError}(\n    '{errorMessage}',\n    {{ cause: error, context: {context} }}\n  );\n}",
          "variables": {
            "mainLogic": "string",
            "operation": "string",
            "context": "string",
            "specificError": "string",
            "specificHandling": "string",
            "customError": "string",
            "errorMessage": "string"
          }
        },
        "dependencyInjection": {
          "pattern": "constructor(\n  @Inject('{tokenName}') private {propertyName}: {interfaceType},\n  {additionalDependencies}\n) {}",
          "variables": {
            "tokenName": "string",
            "propertyName": "string",
            "interfaceType": "string",
            "additionalDependencies": "string"
          }
        }
      }
    },
    "languageSpecific": {
      "typescript": {
        "fileHeader": {
          "pattern": "/**\n * @file {fileName}\n * @purpose {purpose}\n * @ai-context: {{domain: \"{domain}\", layer: \"{layer}\", complexity: \"{complexity}\"}}\n * @dependencies: {dependencies}\n * @exports: {exports}\n */",
          "autoGenerate": true
        },
        "importOptimization": {
          "groupBySource": true,
          "separateThirdParty": true,
          "sortAlphabetically": true
        }
      },
      "javascript": {
        "fileHeader": {
          "pattern": "/**\n * @file {fileName}\n * @purpose {purpose}\n * @ai-context: {{domain: \"{domain}\", layer: \"{layer}\", complexity: \"{complexity}\"}}\n */",
          "autoGenerate": true
        }
      },
      "python": {
        "fileHeader": {
          "pattern": "\"\"\"\n{fileName}\n\nPurpose: {purpose}\nAI Context: {{domain: \"{domain}\", layer: \"{layer}\", complexity: \"{complexity}\"}}\nDependencies: {dependencies}\n\"\"\""
        },
        "functionTemplate": {
          "pattern": "def {functionName}({parameters}) -> {returnType}:\n    \"\"\"\n    Purpose: {purpose}\n    Complexity: {complexity}\n    \n    Args:\n        {args}\n    \n    Returns:\n        {returns}\n    \n    Raises:\n        {raises}\n    \"\"\"\n    # @ai-generated: true\n    {implementation}"
        }
      },
      "java": {
        "classTemplate": {
          "pattern": "/**\n * {purpose}\n * \n * @ai-context {{purpose: \"{contextPurpose}\", pattern: \"{pattern}\", complexity: \"{complexity}\"}}\n * @author AI-Generated\n * @version 1.0\n */\npublic class {className} {\n    {implementation}\n}"
        }
      }
    }
  }
}
